<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <disabledAdministrativeMonitors/>
  <version>1.0</version>
  <numExecutors>2</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>true</useSecurity>
  <authorizationStrategy class="hudson.security.GlobalMatrixAuthorizationStrategy">
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:<%= @settings['jenkins_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:<%= @settings['jenkins_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:<%= @settings['jenkins_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:<%= @settings['jenkins_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:<%= @settings['jenkins_username'] %></permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.sonyericsson.hudson.plugins.gerrit.trigger.PluginImpl.ManualTrigger:<%= @settings['jenkins_username'] %></permission>
    <permission>com.sonyericsson.hudson.plugins.gerrit.trigger.PluginImpl.ManualTrigger:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>com.sonyericsson.hudson.plugins.gerrit.trigger.PluginImpl.Retrigger:<%= @settings['jenkins_username'] %></permission>
    <permission>com.sonyericsson.hudson.plugins.gerrit.trigger.PluginImpl.Retrigger:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Build:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Build:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Configure:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Configure:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Connect:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Connect:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Create:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Create:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Delete:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Delete:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Computer.Disconnect:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Computer.Disconnect:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Hudson.Administer:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Hudson.Read:anonymous</permission>
    <permission>hudson.model.Hudson.Read:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Hudson.Read:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Hudson.RunScripts:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Hudson.RunScripts:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Hudson.UploadPlugins:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Hudson.UploadPlugins:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Build:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Build:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Cancel:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Cancel:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Configure:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Configure:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Create:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Create:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Delete:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Delete:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Discover:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Discover:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Read:anonymous</permission>
    <permission>hudson.model.Item.Read:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Read:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Item.Workspace:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Item.Workspace:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Run.Delete:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Run.Delete:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.Run.Update:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.Run.Update:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.View.Configure:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.View.Configure:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.View.Create:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.View.Create:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.View.Delete:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.View.Delete:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.model.View.Read:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.model.View.Read:<%= @settings['jenkins_admin_username'] %></permission>
    <permission>hudson.scm.SCM.Tag:<%= @settings['jenkins_username'] %></permission>
    <permission>hudson.scm.SCM.Tag:<%= @settings['jenkins_admin_username'] %></permission>
  </authorizationStrategy>
  <securityRealm class="hudson.security.LDAPSecurityRealm" plugin="ldap@1.6">
    <server><%= @settings['ldap_uri'] %></server>
    <rootDN><%= @settings['ldap_root_dn'] %></rootDN>
    <inhibitInferRootDN>false</inhibitInferRootDN>
    <userSearchBase><%= @settings['ldap_userSearchBase'] %></userSearchBase>
    <userSearch><%= @settings['ldap_userSearch'] %></userSearch>
    <managerDN><%= @settings['ldap_managerDN'] %></managerDN>
    <managerPassword><%= @settings['ldap_managerPassword'] %></managerPassword>
    <disableMailAddressResolver>false</disableMailAddressResolver>
  </securityRealm>
  <disableRememberMe>false</disableRememberMe>
  <projectNamingStrategy class="jenkins.model.ProjectNamingStrategy$DefaultProjectNamingStrategy"/>
  <workspaceDir>${ITEM_ROOTDIR}/workspace</workspaceDir>
  <buildsDir>${ITEM_ROOTDIR}/builds</buildsDir>
  <markupFormatter class="hudson.markup.RawHtmlMarkupFormatter">
    <disableSyntaxHighlighting>false</disableSyntaxHighlighting>
  </markupFormatter>
  <jdks/>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <% if @settings['slaves'] -%>
  <slaves>

<% @settings['slaves'].each do |slave| -%>
    <slave>
      <temporaryOfflineCause class="hudson.node_monitors.DiskSpaceMonitorDescriptor$DiskSpace">
        <path>/var/lib/jenkins</path>
        <size>777031680</size>
        <triggered>true</triggered>
        <trigger>hudson.node_monitors.DiskSpaceMonitor</trigger>
      </temporaryOfflineCause>
      <name><%= slave %></name>
      <description></description>
      <remoteFS>/var/lib/jenkins</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.plugins.sshslaves.SSHLauncher" plugin="ssh-slaves@1.5">
        <host><%= slave %></host>
        <port>22</port>
        <credentialsId><%= @settings['credential_id'] %></credentialsId>
      </launcher>
      <label></label>
      <nodeProperties/>
      <userId>jenkins</userId>
    </slave>
<% end -%>
  </slaves>
<% end -%>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
    </hudson.model.AllView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>0</slaveAgentPort>
  <label></label>
  <nodeProperties/>
  <globalNodeProperties/>
</hudson>
